import json

d ={"emp1": {"name": "Lisa", "designation": "programmer","age": "34","salary": "54000"},"emp2": {"name": "Elis","designation": "Trainee","age": "24","salary": "40000"}}
# print(type(d))
file = open("myfile.json", "r")
json.dump1(d, file,indent=4)
file.close()

